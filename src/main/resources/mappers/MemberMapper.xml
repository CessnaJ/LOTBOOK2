<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lotbook.lotbook.repository.MemberMapper">


	<select id="select" parameterType="member" resultType="lotbook.lotbook.dto.entity.Member">
		SELECT * FROM member WHERE email=#{email}
	</select>

	<select id="selectMemberBySequence" parameterType="int" resultType="lotbook.lotbook.dto.entity.Member">
		SELECT * FROM member WHERE sequence=#{sequence}
	</select>


</mapper>

